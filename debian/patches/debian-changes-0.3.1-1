+ */
+class Phorms_Utilities_AutoLoader
+{
+    /**
+     * The extension to use when locating class names.
+     * 
+     * @var    string
+     * @access protected
+     * @static
+     */
+    protected static $extension = '.php';
+    
+    /**
+     * A flag to indicate if this auto loader has been registered.
+     *
+     * @var    string
+     * @access protected
+     * @static
+     */
