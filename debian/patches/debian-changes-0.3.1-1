+        }
+        return $this->_listener->endElement(strtolower($matches[1]));
+    }
+    
+    /**
+     *    Part of the tag data.
+     *    @param string $token     Incoming characters.
+     *    @param integer $event    Lexer event type.
+     *    @return boolean          False if parse error.
+     *    @access public
+     */
