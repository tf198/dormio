+        $this-&gt;assertTitle('My Home Page');
+        $this-&gt;clickLink('Contact');
+        $this-&gt;assertTitle('Contact me');
+        $this-&gt;assertWantedPattern('/Email me at/');
+    }</strong>
+}
+</pre>
+            </p>
+        
+        <p><a class="target" name="fonctionnalites"><h2>Liste des fonctionnalites</h2></a></p>
