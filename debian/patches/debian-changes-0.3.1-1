+    {
+        $this->assertNotNull(new $this->field_class($this->test_label));
+    }
+    
+    /**
+     * Tests the help text of the field.
+     *
+     * @access public
+     * @return void
+     */
+    public function testHelpText()
+    {
+        $field = new $this->field_class($this->test_label, $this->test_help);
+        
