+        $this->multi_part = $multi_part;
+        if ($this->multi_part && $method != Phorms_Forms_Form::POST) {
+            $method = Phorms_Forms_Form::POST;
+            trigger_error('Multi-part form method changed to POST.', E_USER_WARNING);
+        }
